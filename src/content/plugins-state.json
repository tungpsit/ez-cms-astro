{
  "analytics": {
    "enabled": true,
    "settings": {
      "trackPageViews": true,
      "trackClicks": false,
      "anonymizeIP": true
    }
  },
  "code-highlighter": {
    "enabled": false,
    "settings": {
      "theme": "dracula",
      "lineNumbers": true,
      "copyButton": true
    }
  },
  "comments": {
    "enabled": true,
    "settings": {
      "moderation": true,
      "allowAnonymous": false,
      "nestingLevel": 3
    }
  },
  "newsletter": {
    "enabled": true,
    "settings": {
      "provider": "internal",
      "doubleOptIn": true,
      "welcomeEmail": true
    }
  },
  "seo-optimizer": {
    "enabled": true,
    "settings": {
      "autoGenerateMetaTags": true,
      "generateSitemap": true,
      "enableOpenGraph": true
    }
  },
  "social-sharing": {
    "enabled": true,
    "settings": {
      "platforms": [
        "twitter",
        "facebook",
        "linkedin"
      ],
      "position": "bottom"
    }
  }
}