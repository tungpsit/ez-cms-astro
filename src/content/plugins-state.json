{
  "analytics": {
    "enabled": true,
    "settings": {
      "trackPageViews": true,
      "trackClicks": false,
      "anonymizeIP": true
    }
  },
  "code-highlighter": {
    "enabled": false,
    "settings": {
      "theme": "dracula",
      "lineNumbers": true,
      "copyButton": true
    }
  },
  "comments": {
    "enabled": true,
    "settings": {
      "moderation": true,
      "allowAnonymous": false,
      "nestingLevel": 3
    }
  },
  "contact-form": {
    "enabled": true,
    "settings": {
      "notificationEmail": "",
      "smtpHost": "",
      "smtpPort": 587,
      "smtpUser": "",
      "smtpPassword": "",
      "smtpSecure": false,
      "successMessage": "Thank you for your message. We will get back to you soon!",
      "errorMessage": "There was an error submitting your form. Please try again.",
      "enableRecaptcha": false,
      "recaptchaSiteKey": "",
      "recaptchaSecretKey": ""
    }
  },
  "newsletter": {
    "enabled": true,
    "settings": {
      "provider": "internal",
      "doubleOptIn": true,
      "welcomeEmail": true
    }
  },
  "seo-optimizer": {
    "enabled": true,
    "settings": {
      "autoGenerateMetaTags": true,
      "generateSitemap": true,
      "enableOpenGraph": true
    }
  },
  "social-sharing": {
    "enabled": true,
    "settings": {
      "platforms": [
        "twitter",
        "facebook",
        "linkedin"
      ],
      "position": "bottom"
    }
  }
}